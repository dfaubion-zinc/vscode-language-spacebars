{
  "name": "spacebars",
  "version": "0.2.3",
  "repository": {
    "type": "git",
    "url": "https://github.com/tamino-martinius/vscode-language-spacebars"
  },
  "license": "MIT",
  "publisher": "tamino-martinius",
  "engines": {
    "vscode": "0.10.x"
  },
  "icon": "images/icon.png",
  "categories": [
    "Programming Languages",
    "Snippets"
  ],
  "contributes": {
    "languages": [{
      "id": "spacebars",
      "extensions": [".html", ".handlebars", ".hbs", ".tpl"],
      "aliases": ["Spacebars", "spacebars"],
      "mimetypes": ["text/x-spacebars-template"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "spacebars",
      "scopeName": "text.html.spacebars",
      "path": "./syntaxes/Spacebars.tmLanguage.json"
    }],
    "snippets": [{
      "language": "spacebars",
      "path": "./snippets/snippets.json"
    }]
  }
}
